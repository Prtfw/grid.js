{
  "name": "grid.js",
  "version": "0.0.1",
  "description": "A Node socket API supporting syft.js workers in the browser",
  "main": "src/index.js",
  "scripts": {
    "start": "VERBOSE=true nodemon --exec babel-node src/index.js",
    "build": "babel src --out-dir dist",
    "serve": "node dist/index.js",
    "test": "jest --coverage --maxWorkers 4",
    "test:watch": "jest --watch",
    "seed": "./seed/run-seed.sh",
    "create-seed": "python ./seed/create-seed.py"
  },
  "engines": {
    "node": "10.16.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmined/grid.js.git"
  },
  "author": "OpenMined",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/openmined/grid.js/issues"
  },
  "homepage": "https://github.com/openmined/grid.js#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "bcrypt": "^3.0.7",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.3.1",
    "redis": "^2.8.0",
    "syft.js": "git+https://github.com/openmined/syft.js.git",
    "uuid": "^3.3.2",
    "ws": "^7.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-core": "^6.26.3",
    "husky": "^3.0.3",
    "jest": "^24.8.0",
    "mock-socket": "^9.0.0",
    "mongodb-memory-server": "^5.2.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "redis-mock": "^0.46.0"
  }
}
